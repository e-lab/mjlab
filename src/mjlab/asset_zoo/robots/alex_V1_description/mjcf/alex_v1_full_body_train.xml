        <position name="left_wrist_x"  joint="left_wrist_x"  class="alex/wrist/x"/>
        <!-- Neck / head -->
        <position name="neck_z" joint="neck_z" class="alex/neck/z"/>
        <position name="neck_y" joint="neck_y" class="alex/neck/y"/>
        <!-- Right arm -->
        <position name="right_shoulder_y" joint="right_shoulder_y" class="alex/shoulder/y"/>
        <position name="right_shoulder_x" joint="right_shoulder_x" class="alex/shoulder/x"/>
