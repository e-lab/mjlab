         <!-- Right Thight IMU: -->
        <gyro name="imu_right_thigh_gyro" site="imu_right_thigh"/>
        <accelerometer name="imu_right_thigh_accelerometer" site="imu_right_thigh"/>

        <!-- Right Shin IMU: -->
