        <position name="left_hip_y"   joint="left_hip_y"   class="alex/hip/y"/>
        <position name="left_knee"    joint="left_knee_y"  class="alex/knee"/>
        <position name="left_ankle_y" joint="left_ankle_y" class="alex/ankle/y"/>
        <position name="left_ankle_x" joint="left_ankle_x" class="alex/ankle/x"/>
        <position name="right_hip_x"   joint="right_hip_x"   class="alex/hip/x"/>
        <position name="right_hip_z"   joint="right_hip_z"   class="alex/hip/z"/>
        <position name="right_hip_y"   joint="right_hip_y"   class="alex/hip/y"/>
        <position name="right_knee"    joint="right_knee_y"  class="alex/knee"/>
        <position name="right_ankle_y" joint="right_ankle_y" class="alex/ankle/y"/>
        <position name="right_ankle_x" joint="right_ankle_x" class="alex/ankle/x"/>
