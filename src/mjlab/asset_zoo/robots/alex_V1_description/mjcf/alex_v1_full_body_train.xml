        <accelerometer name="imu_right_shin_accelerometer" site="imu_right_shin"/>

        <!-- Right Foot IMU: -->
        <gyro name="imu_right_foot_gyro" site="imu_right_foot"/>
        <accelerometer name="imu_right_foot_accelerometer" site="imu_right_foot"/>

        <!-- Torso IMU: -->
        <gyro name="imu_torso_gyro" site="imu_torso"/>
        <accelerometer name="imu_torso_accelerometer" site="imu_torso"/>

        <!-- Head IMU: -->
        <gyro name="imu_head_gyro" site="imu_head"/>
        <accelerometer name="imu_head_accelerometer" site="imu_head"/>

        <!-- Left Shoulder Pitch IMU: -->
        <gyro name="imu_left_shoulder_y_gyro" site="imu_left_shoulder_y"/>
        <accelerometer name="imu_left_shoulder_y_accelerometer" site="imu_left_shoulder_y"/>

        <!-- Left Shoulder Yaw IMU: -->
        <gyro name="imu_left_shoulder_z_gyro" site="imu_left_shoulder_z"/>
        <accelerometer name="imu_left_shoulder_z_accelerometer" site="imu_left_shoulder_z"/>

        <!-- Left Wrist Yaw IMU: -->
        <gyro name="imu_left_wrist_z_gyro" site="imu_left_wrist_z"/>
        <accelerometer name="imu_left_wrist_z_accelerometer" site="imu_left_wrist_z"/>
        
        <!-- Right Shoulder Pitch IMU: -->
        <gyro name="imu_right_shoulder_y_gyro" site="imu_right_shoulder_y"/>
        <accelerometer name="imu_right_shoulder_y_accelerometer" site="imu_right_shoulder_y"/>

        <!-- Right Shoulder Yaw IMU: -->
        <gyro name="imu_right_shoulder_z_gyro" site="imu_right_shoulder_z"/>
        <accelerometer name="imu_right_shoulder_z_accelerometer" site="imu_right_shoulder_z"/>

        <!-- Right Wrist Yaw IMU: -->
        <gyro name="imu_right_wrist_z_gyro" site="imu_right_wrist_z"/>
        <accelerometer name="imu_right_wrist_z_accelerometer" site="imu_right_wrist_z"/>

        <!-- Additional Sensors for training: -->
        <!-- Pelvis IMU: -->
        <framequat name="imu_pelvis_orientation" objtype="site" objname="imu_pelvis"/>
        <velocimeter name="imu_pelvis_linear_velocity" site="imu_pelvis"/>
        <framepos name="imu_pelvis_position" objtype="site" objname="imu_pelvis"/>

        <!-- Globals: -->
        <framezaxis name="upvector" objtype="site" objname="imu_pelvis"/>
        <framexaxis name="forwardvector" objtype="site" objname="imu_pelvis"/>
        <framelinvel name="global_linear_velocity" objtype="site" objname="imu_pelvis"/>
        <frameangvel name="global_angular_velocity" objtype="site" objname="imu_pelvis"/>
        <framelinvel name="left_foot_global_linear_velocity" objtype="site" objname="left_foot_site"/>
        <framelinvel name="right_foot_global_linear_velocity" objtype="site" objname="right_foot_site"/>
    </sensor>

